# Copy to .env and set values. Required for docker compose.
# Generate secrets: openssl rand -base64 40 (PG_PASS max 99 chars)
#                    openssl rand -base64 50 (AUTHENTIK_SECRET_KEY)

PG_PASS=
AUTHENTIK_SECRET_KEY=

